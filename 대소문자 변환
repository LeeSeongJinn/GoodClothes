#include <stdio.h>
#define LEN_INPUT 10

int main(void) {
    char s1[LEN_INPUT];
    scanf("%s", &s1);
    int su=strlen(s1);
    for(int i=0;i<=su;i++){
        if(64<s1[i]&&s1[i]<91){
            printf("%c",s1[i]+32);
        }
        else if(96<s1[i]&&s1[i]<123){
            printf("%c",s1[i]-32);
        }
        
        
    }

    return 0;
}
